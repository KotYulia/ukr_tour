<% if current_user&.admin? %>
  <h1>Users:</h1>
  <ul>
    <% @users.each do |user| %>
      <li><%= link_to user.full_name, user %></li>
    <% end %>
  </ul>
<% elsif current_user&.standard?
     controller.redirect_to user_path(current_user)%>
<% else
   controller.redirect_to '/login'%>
<% end %>